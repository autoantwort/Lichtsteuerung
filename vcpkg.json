{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "lichtsteuerung",
    "version-string": "2.0.0",
    "dependencies": [
        "boost-coroutine",
        "boost-coroutine2",
        "boost-stacktrace",
        {
          "name": "libbacktrace",
          "platform": "!(windows & !mingw)"
        },
        "span-lite",
        "rtaudio",
        "kubazip",
        {
            "name": "aubio",
            "default-features": false
        },
        "qtquickcontrols2",
        "qtmqtt",
        "qtnetworkauth",
        "qtwebsockets",
        "qt5compat",
        "qtsvg",
        "segvcatch"
    ],
    "vcpkg-configuration": {
        "overlay-ports": [
            "overlay-ports"
        ]
    }
}
