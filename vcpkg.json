{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "name": "lichtsteuerung",
    "version-string": "2.0.0",
    "builtin-baseline": "80d54ff62d528339c626a6fbc3489a7f25956ade",
    "dependencies": [
        "boost-coroutine",
        "boost-coroutine2",
        "boost-stacktrace",
        {
            "name": "libbacktrace",
            "platform": "!(windows & !mingw)"
        },
        "span-lite",
        "rtaudio",
        "kubazip",
        {
            "name": "aubio",
            "default-features": false
        },
        "segvcatch",
        {
            "name": "alsa",
            "platform": "linux"
        },
        {
            "name": "susbdmx",
            "platform": "windows"
        },
        {
            "name": "awlib",
            "default-features": false,
            "features": [
                "graphics"
            ]
        }
    ],
    "features": {
        "qt": {
            "description": "Used qt modules via vcpkg",
            "dependencies": [
                "qtquickcontrols2",
                "qtmqtt",
                "qtnetworkauth",
                "qtwebsockets",
                "qt5compat",
                "qtsvg"
            ]
        }
    },
    "vcpkg-configuration": {
        "overlay-ports": [
            "overlay-ports"
        ]
    }
}